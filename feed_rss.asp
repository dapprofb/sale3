<script type="text/javascript">
    function createDynamicRssFeed() {
        var keyword = document.getElementById('keyword').value;
        if (keyword != '') {
            if (document.getElementById("exact").checked == true) {
                keyword = keyword.replace(/ /g, "-");
            }
            if (document.getElementById("both").checked == true) {
                keyword = keyword.replace(/ /g, "+");
            }
            if (document.getElementById("any").checked == true) {
                keyword = keyword.replace(/ /g, "_");
            }
            window.open('https://www.studiocataldi.it/news/rss-tag.asp?id=' + keyword); 
         }
    }
</script>
