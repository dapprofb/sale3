    //cell5.innerHTML = "<input type='text' class='tribunale' size='4' id='chargesNoTax2" + rowCount + "' name='chargesNoTax2" + rowCount + "' value='' />";
    cell5.innerHTML = "<input type='text' class='tribunale' size='4' id='quantity" + rowCount + "' name='quantity" + rowCount + "' value='' />";
    document.getElementById("fieldsNum").value = rowCount;
}
